{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Created ads info",
    "type": "object",
    "properties": {
        "title": {
            "type": "string"
        },
        "price": {
            "type": "number"
        },
        "available": {
            "type": "number"
        },
        "deliveryMethod": {
            "type": "string",
            "enum": ["collect", "freight"]
        },
        "outlet": {
            "type": "string"
        },
        "state": {
            "type": "string",
            "enum": ["active", "inactive"]
        },
        "type": {
            "type": "string",
            "enum": ["buy", "sell"]
        },
        "date": {
            "type": "string",
            "format": "date-time"
        }
    },
    "required": ["title", "price", "available", "deliveryMethod", "outlet", "state", "status", "type", "date"]
}

